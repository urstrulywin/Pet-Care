generator client {
  provider = "prisma-client"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
}

model Pet {
  id        String   @id @default(cuid())
  name      String
  ownerName String
  imageUrl  String
  age       Int
  notes     String
  updatedAt DateTime @updatedAt
  createdAt DateTime @default(now())
}
